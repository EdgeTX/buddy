    "react-dom": "^17.0.2",
    "react-router-dom": "^6.0.2",
    "semver": "^7.3.5",
    "source-map-support": "^0.5.21",
    "styled-components": "^5.3.3",
    "type-guards": "^0.15.0",
    "unzipit": "^1.3.6",
    "usb": "2.0.3",
    "use-media": "^1.4.0",
    "uuid": "^8.3.2",
    "web-streams-polyfill": "^3.2.0"
  },
  "scripts": {
    "start": "yarn compile:main && yarn start:renderer",
    "start:mocked": "yarn compile:main && MOCKED=true yarn start:renderer",
    "start:renderer": "webpack-dev-server --hot --mode development --config webpack/renderer.config.js",
    "start:web": "WEB_ONLY=true yarn start:renderer",
    "start:cors": "node cors.js",
    "e2e:web": "playwright test --config=tests/config/default.config.ts --project=chromium",
    "e2e:electron": "playwright test --config=tests/config/electron.config.ts --project=chromium",
    "test": "node --experimental-vm-modules --no-warnings node_modules/jest/bin/jest.js",
    "test:types": "tsc -p tsconfig.spec.json",
    "build": "yarn compile:production && yarn run pack",
    "pack": "electron-builder --publish never",
    "compile": "yarn run compile:deps && webpack",
    "compile:production": "yarn compile --mode production",
    "compile:web": "webpack --config webpack/renderer.config.js",
    "compile:web:production": "yarn compile:web --mode production",
    "compile:main": "yarn run compile:deps && webpack --config webpack/main.config.js",
    "compile:deps": "electron-rebuild",
    "codegen": "graphql-codegen --config codegen.yml",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx",
    "lint:all": "yarn lint src",
    "fmt": "prettier --write '**/*.{ts,tsx,js,jsx}'",
    "fmt:check": "prettier --check '**/*.{ts,tsx,js,jsx}'",
    "postinstall": "yarn codegen && husky install",
    "storybook": "start-storybook -p 6006",
    "build:storybook": "build-storybook",
    "release": "electron-builder --publish always",
    "serve:web": "serve build/renderer -p 8081"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "eslint --fix"
    ],
    "**/*.{json,js,jsx,ts,tsx}": [
      "prettier --write"
    ]
  },
