  "name": "edgetx-buddy",
  "version": "0.1.0",
  "private": true,
  "license": "MIT",
  "main": "./build/main/main.js",
  "build": {
    "productName": "EdgeTX Buddy",
    "appId": "org.edgetx.buddy",
    "mac": {
      "publish": [
        "github"
      ],
      "category": "your.app.category.type",
      "artifactName": "edgetx-buddy-${version}.${ext}"
    },
    "linux": {
      "publish": [
        "github"
      ],
      "category": "Utility",
      "executableName": "edgetx-buddy",
      "artifactName": "edgetx-buddy-${version}.${ext}"
    },
    "win": {
      "publish": [
        "github"
      ],
      "target": "msi",
      "artifactName": "edgetx-buddy-${version}.${ext}"
    },
    "files": [
      "!**/*",
      "./build/main/*",
      "./build/renderer/index.html",
      "./build/renderer/main-*.js",
      "./build/renderer/main-*.map",
      "./build/renderer/locales/*"
    ],
    "npmRebuild": false,
    "directories": {
      "buildResources": "assets"
    }
  },
